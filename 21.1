#include <stdio.h>
int main() {
    int num, first, last, temp, digits = 1;

    printf("Enter a number: ");
    scanf("%d", &num);
    temp = num;
    last = num % 10;
    while (temp >= 10) {
        temp = temp / 10;
        digits = digits * 10;
    }
    first =temp;
    if (num < 10) {
        printf("Swapped number = %d", num);
    } else {
        num = num % digits;        // remove first digit
        num = num / 10;            // remove last digit
        num = last * digits + num * 10 + first;
        printf("Swapped number = %d", num);
    }

    return 0;
}
